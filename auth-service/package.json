{
    "name": "instagram-backend",
    "version": "1.0.0",
    "description": "Backend for instagram clone. ",
    "type": "commonjs",
    "keywords": [
        "Instagram",
        "Clone",
        "Social",
        "Media"
    ],
    "license": "MIT",
    "author": "Aman Khadka",
    "main": "./index.ts",
    "scripts": {
        "start": "nodemon",
        "dev": "nodemon",
        "build": "tsc",
        "typeorm": "typeorm-ts-node-commonjs",
        "migration:create": "npm run typeorm migration:create",
        "migration:generate": "npm run build && npm run typeorm migration:generate -- -d dist/src/config/db.config.js",
        "migration:run": "npm run build && npm run typeorm migration:run -- -d dist/src/config/db.config.js",
        "migration:revert": "npm run build && npm run typeorm migration:revert -- -d dist/src/config/db.config.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:unit": "jest --config test/jest-unit.config.json",
        "test:unit:watch": "jest --watch --config test/jest-unit.config.json",
        "test:unit:file": "jest --config test/jest-unit.config.json --testPathPattern",
        "test:unit:cov": "jest --coverage --config test/jest-unit.config.json",
        "test:e2e": "jest --config test/jest-e2e.config.json",
        "test:e2e:watch": "jest --watch --config test/jest-e2e.config.json",
        "test:e2e:file": "jest --config test/jest-e2e.config.json --testPathPattern",
        "test:e2e:cov": "jest --coverage --config test/jest-e2e.config.json"
    },
    "dependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/swagger-ui-express": "^4.1.8",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.3",
        "compression": "^1.8.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^4.21.2",
        "helmet": "^8.0.0",
        "joi": "^17.13.3",
        "joi-password-complexity": "^5.2.0",
        "jsonwebtoken": "^9.0.2",
        "kafkajs": "^2.2.4",
        "module-alias": "^2.2.3",
        "node-forge": "^1.3.1",
        "pg": "^8.13.3",
        "redis": "^4.7.0",
        "swagger-autogen": "^2.23.7",
        "swagger-ui-express": "^5.0.1",
        "typeorm": "^0.3.21",
        "uuid": "^11.1.0",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@babel/core": "^7.27.4",
        "@babel/preset-env": "^7.27.2",
        "@babel/preset-typescript": "^7.27.1",
        "@eslint/js": "^9.29.0",
        "@types/body-parser": "^1.19.5",
        "@types/compression": "^1.7.5",
        "@types/cookie-parser": "^1.4.8",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/module-alias": "^2.0.4",
        "@types/node": "^22.13.10",
        "@types/node-forge": "^1.3.11",
        "@types/supertest": "^6.0.2",
        "@types/uuid": "^10.0.0",
        "babel-jest": "^30.0.2",
        "eslint": "^9.29.0",
        "globals": "^16.2.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.9",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.6",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.34.1"
    }
}
